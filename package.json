      "@types/mocha": "^5.2.5",
      "@types/node": "^10.12.8",
      "chai": "^4.2.0",
      "coveralls": "^3.0.2",
      "mocha": "^5.2.0",
      "nyc": "^13.1.0",
      "rollup-plugin-node-resolve": "^3.4.0",
      "tslint": "^5.11.0",
      "typescript": "^3.1.6"
   }
